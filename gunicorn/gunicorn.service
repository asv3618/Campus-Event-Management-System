[Unit]
Description=gunicorn daemon
Requires=gunicorn.socket
After=network.target
[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/home/ubuntu/CampusEventsManagementSystem - Copy
ExecStart=/home/ubuntu/env/bin/gunicorn --access-logfile - --workers 3 --bind unix:/home/ubuntu/CampusEventsManagementSystem - Copy/CampusEventsManagementSystem/CampusEventsManagementSystem.sock CampusEventsManagementSystem.wsgi:application
       
[Install]
WantedBy=multi-user.target